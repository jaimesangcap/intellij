<application>
  <component name="ReplCommandManager">
    <repl-command name="(shadow/repl :app)" executionType="EXPRESSION" commandText="(shadow.cljs.devtools.api/repl :app)" executeBefore="NONE" executionNs="REPL_CURRENT" inNamespace="" saveAll="false" />
    <repl-command name="user/reset" executionType="EXPRESSION" commandText="(reset)" executeBefore="NONE" executionNs="EXPLICIT_NS" inNamespace="user" saveAll="true" />
    <repl-command name="kaocha.repl/run" executionType="EXPRESSION" commandText="(krepl/run '~current-test-var)" executeBefore="SYNC_ALL" executionNs="EXPLICIT_NS" inNamespace="dev" saveAll="true" />
    <repl-command name="kaocha.repl/run-all" executionType="EXPRESSION" commandText="(krepl/run-all)" executeBefore="SYNC_ALL" executionNs="EXPLICIT_NS" inNamespace="dev" saveAll="true" />
    <repl-command name="add-libs" executionType="EXPRESSION" commandText="(add-libs '~selection)" executeBefore="NONE" executionNs="EXPLICIT_NS" inNamespace="clojure.tools.deps.alpha.repl" saveAll="true" />
    <repl-command name="shadow-watch-compile-all" executionType="EXPRESSION" commandText="(shadow.cljs.devtools.api/watch-compile-all!)" executeBefore="NONE" executionNs="EXPLICIT_NS" inNamespace="shadow.user" saveAll="true" />
    <repl-command name="shadow watch" executionType="EXPRESSION" commandText="(shadow.cljs.devtools.api/watch :app {:autobuild false})" executeBefore="NONE" executionNs="EXPLICIT_NS" inNamespace="shadow.user" saveAll="true" />
    <repl-command name="tap-sel" executionType="EXPRESSION" commandText="(tap&gt; ~selected-form)" executeBefore="NONE" executionNs="REPL_CURRENT" inNamespace="" saveAll="true" />
    <repl-command name="tap-top" executionType="EXPRESSION" commandText="(tap&gt; ~top-level-form)" executeBefore="NONE" executionNs="REPL_CURRENT" inNamespace="" saveAll="true" />
    <repl-command name="dev/reset" executionType="EXPRESSION" commandText="(reset)" executeBefore="NONE" executionNs="EXPLICIT_NS" inNamespace="dev" saveAll="true" />
    <repl-command name="cljs.test/run-tests" executionType="EXPRESSION" commandText="(cljs.test/run-tests '~current-test-var)" executeBefore="NONE" executionNs="REPL_CURRENT" inNamespace="" saveAll="true" />
    <repl-command name="dev/go" executionType="EXPRESSION" commandText="(go)" executeBefore="NONE" executionNs="EXPLICIT_NS" inNamespace="dev" saveAll="true" />
  </component>
</application>